{
  "name": "CatBlock",
  "version": "1.0",
  "description": "I can't has cheezburger!",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "https://*/*",
    "http://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ]
  },
  "manifest_version": 2
}